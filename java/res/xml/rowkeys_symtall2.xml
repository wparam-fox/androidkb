<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.wparam.kb.inputmethod.latin"
>
    <Key
        latin:keyWidth="14%p"
        latin:visualInsetsLeft="1%p"
        latin:keyLabel="&lt;"
        latin:code="!raw/0x15"
        latin:keyActionFlags="isRepeatable|noKeyPreview"
         />
 <Key
        latin:keyWidth="13%p"
        latin:keyLabel="&#x2022;"
        latin:code="!raw/0x17"
	/>
<Key
        latin:keyWidth="13%p"
        latin:keyLabel="&gt;"
        latin:code="!raw/0x16"
        latin:keyActionFlags="isRepeatable|noKeyPreview"
         />
    <Key
        latin:keyLabel="^Z"
        latin:code="!craw/0x36"
        latin:keyWidth="12%p"
        />
    <Key
        latin:keyLabel="^X"
        latin:code="!craw/0x34"
        latin:keyWidth="12%p"
        />
    <Key
        latin:keyLabel="^C"
        latin:code="!craw/0x1f"
        latin:keyWidth="12%p"
        />
    <Key
        latin:keyLabel="^V"
        latin:code="!craw/0x32"
        latin:keyWidth="12%p"
        />
    <Key
        latin:keyLabel="^F"
        latin:code="!craw/0x22"
        latin:keyWidth="12%p"
        />

    
</merge>
