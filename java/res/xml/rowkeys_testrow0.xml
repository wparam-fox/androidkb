<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.wparam.kb.inputmethod.latin"
>
    <Key
        latin:keyLabel="1"
        latin:keyHintLabel="!"
        latin:additionalMoreKeys="!"
        />
    <Key
        latin:keyLabel="v"
        latin:code="!raw/0x14"
        latin:keyActionFlags="isRepeatable|noKeyPreview"
         />
    <Key
        latin:keyLabel="&lt;"
        latin:code="!raw/0x15"
        latin:keyActionFlags="isRepeatable|noKeyPreview"
        />
    <Key
        latin:keyLabel="&gt;"
        latin:code="!raw/0x16"
        latin:keyActionFlags="isRepeatable|noKeyPreview"
        />
    <Key
        latin:keyLabel="^"
        latin:code="!raw/0x13"
        latin:keyActionFlags="isRepeatable|noKeyPreview"
        />
    <Key
        latin:keyLabel="6"
        latin:keyHintLabel="^"
        latin:additionalMoreKeys="^"
        />
    <Key
        latin:keyLabel="7"
        latin:keyHintLabel="&amp;"
        latin:additionalMoreKeys="&amp;"
        />
    <Key
        latin:keyLabel="8"
        latin:keyHintLabel="*"
        latin:additionalMoreKeys="*"
        />
    <Key
        latin:keyLabel="9"
        latin:keyHintLabel="("
        latin:additionalMoreKeys="("
        />
    <Key
        latin:keyLabel="0"
        latin:keyHintLabel=")"
        latin:additionalMoreKeys=")" />
</merge>
