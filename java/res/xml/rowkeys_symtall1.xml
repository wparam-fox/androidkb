<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.wparam.kb.inputmethod.latin"
>
    <Key
        latin:keyWidth="40%p"
        latin:visualInsetsRight="13%p"
        latin:visualInsetsLeft="14%p"
        latin:keyLabel="^"
        latin:code="!raw/0x13"
        latin:keyActionFlags="isRepeatable|noKeyPreview"
         />
    <Key
        latin:keyLabel="^A"
        latin:code="!craw/0x1d"
        latin:keyWidth="12%p"
        />
    <Key
        latin:keyLabel="^E"
        latin:code="!craw/0x21"
        latin:keyWidth="12%p"
        />
    <Key
        latin:keyLabel="^W"
        latin:code="!craw/0x33"
        latin:keyWidth="12%p"
        />
    <Key
        latin:keyLabel="^U"
        latin:code="!craw/0x31"
        latin:keyWidth="12%p"
        />
    <Key
        latin:keyLabel="^B"
        latin:code="!craw/0x1e"
        latin:keyWidth="12%p"
        />
    
    
</merge>
