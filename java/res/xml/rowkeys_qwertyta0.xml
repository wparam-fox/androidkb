<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin"
>
    <Key
        latin:keyLabel="1"
        latin:keyHintLabel="!"
        latin:additionalMoreKeys="!"
        />
    <Key
        latin:keyLabel="2"
        latin:keyHintLabel="\@"
        latin:additionalMoreKeys="\@"
         />
    <Key
        latin:keyLabel="3"
        latin:keyHintLabel="#"
        latin:additionalMoreKeys="#"
        />
    <Key
        latin:keyLabel="4"
        latin:keyHintLabel="$"
        latin:additionalMoreKeys="$"
        />
    <Key
        latin:keyLabel="5"
        latin:keyHintLabel="%"
        latin:additionalMoreKeys="%"
        />
    <Key
        latin:keyLabel="6"
        latin:keyHintLabel="^"
        latin:additionalMoreKeys="^"
        />
    <Key
        latin:keyLabel="7"
        latin:keyHintLabel="&amp;"
        latin:additionalMoreKeys="&amp;"
        />
    <Key
        latin:keyLabel="8"
        latin:keyHintLabel="*"
        latin:additionalMoreKeys="*"
        />
    <Key
        latin:keyLabel="9"
        latin:keyHintLabel="("
        latin:additionalMoreKeys="("
        />
    <Key
        latin:keyLabel="0"
        latin:keyHintLabel=")"
        latin:additionalMoreKeys=")" />
</merge>
