<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.wparam.kb.inputmethod.latin"
>
    <Key
        latin:keyLabel="1"
        latin:keyHintLabel="!"
        latin:additionalMoreKeys="!,&#x00A1;"
        />
    <Key
        latin:keyLabel="2"
        latin:keyHintLabel="\@"
        latin:additionalMoreKeys="\@"
         />
    <Key
        latin:keyLabel="3"
        latin:keyHintLabel="#"
        latin:additionalMoreKeys="#"
        />
    <Key
        latin:keyLabel="4"
        latin:keyHintLabel="$"
        latin:additionalMoreKeys="$,&#x00A3;,&#x00A2;,&#x20AC;,&#x00A5;,&#x20B1;"
        />
    <Key
        latin:keyLabel="5"
        latin:keyHintLabel="%"
        latin:additionalMoreKeys="%,&#x2030;"
        />
    <Key
        latin:keyLabel="6"
        latin:keyHintLabel="^"
        latin:additionalMoreKeys="^,&#x2191;,&#x2193;,&#x2190;,&#x2192;"
        />
    <Key
        latin:keyLabel="7"
        latin:keyHintLabel="&amp;"
        latin:additionalMoreKeys="&amp;"
        />
    <Key
        latin:keyLabel="8"
        latin:keyHintLabel="*"
        latin:additionalMoreKeys="*"
        />
    <Key
        latin:keyLabel="9"
        latin:keyHintLabel="("
        latin:additionalMoreKeys="("
        />
    <Key
        latin:keyLabel="0"
        latin:keyHintLabel=")"
        latin:additionalMoreKeys=")" />
</merge>
