<merge xmlns:latin="http://schemas.android.com/apk/res/com.wparam.kb.inputmethod.latin" >

    <Key
        latin:keyWidth="13%p"
        latin:keyLabelFlags="hasPopupHint|preserveCase"
        latin:keyLabel="bob"
        latin:moreKeys="http://,ftp://,irc://,file://"
        latin:backgroundType="functional"
        latin:keyOutputText="https://"
        />
    <Key
        latin:keyWidth="13%p"
        latin:keyLabelFlags="hasPopupHint|preserveCase"
        latin:keyLabel="adoo"
        latin:moreKeys="http://,ftp://,irc://,file://"
        latin:backgroundType="functional"
        latin:keyOutputText="https://"
        />

    <include
        latin:keyboardLayout="@xml/key_space_symbols" />

    <Key
        latin:keyWidth="14%p"
        latin:keyLabelFlags="hasPopupHint|preserveCase"
        latin:keyLabel="https"
        latin:moreKeys="!fixedColumnOrder!4,http://,ftp://,irc://,file://"
        latin:backgroundType="functional"
        latin:keyOutputText="https://"
        />
    <include
        latin:keyboardLayout="@xml/key_f2" />

</merge>
